# ~/.ssh/config: ssh_config(5)
# vim: ft=sshconfig

# note that the '#if' directives are not supported by any SSH client directly;
# instead, they are processed by the ssh/generate script.

#if OpenSSH && Version 6.3
IgnoreUnknown CanonicalDomains,CanonicalizeHostname,PubkeyAcceptedKeyTypes
#endif

# work

Host cacti cacti.utenos-kolegija.lt
	HostName uk-cacti.utenos-kolegija.lt

Host *.utenos-kolegija.lt *.ukolegija.lt 193.219.181.* !193.219.181.219 !193.219.181.229
	IdentityFile ~/.ssh/work/id_ukolegija
	ForwardAgent yes

Host gw-b56-bypass
	HostName 193.219.181.189
	HostkeyAlias gw-b56.utenos-kolegija.lt

Host gw-m7-bypass
	HostName 193.219.181.206
	ProxyJump gw-b56-bypass

Host radius-bypass
	HostName 193.219.181.253
	ProxyJump gw-b56-bypass

# domain-wide settings

Host theos
	HostName %h.kyriasis.com

Host blizzard.nullroute.eu.org
	User mantas

#if Host star || Host ember || Host wind
# 2020-04-04: Hack to improve star->{ember,wind} throughput
#             (which is ~900 Mbps over IPv4, but only ~220 Mbps
#             over IPv6 due to lack of peering between ISPs)
Host ember ember.nullroute.eu.org
	AddressFamily inet
Host wind wind.nullroute.eu.org
	AddressFamily inet
Host star star.nullroute.eu.org
	AddressFamily inet
#endif

Host *.nullroute.eu.org
	GSSAPIDelegateCredentials yes
	ForwardAgent yes

Host *.zt1
	HostName %h.nullroute.eu.org

Host *.home *.local 10.* 192.168.*
	ForwardAgent yes
	UserKnownHostsFile ~/.ssh/known_hosts.local

# other hosts

Host aur
	HostName aur.archlinux.org
	User aur

Host eisner
	HostName eisner.encompasserve.org
	User mikulenas

Host github.com
	ControlMaster auto
	ControlPersist 5m

Host ftp.ext.hpe.com
	Port 2222
	# 2019-03-21: Server includes umac-64@openssh.com at the end of
	# its proposal, but hangs after MSG_NEWKEYS if it is selected.
	MACs -umac-*
	PreferredAuthentications password

Host *.oraclecloud.com
	ControlPath ~/.ssh/S.%C.oraclecloud.com:%p

Host ocid*.instance.oc*.*.*
	ControlPath ~/.ssh/S.%C.oraclecloud.com:%p

Host sdf sdf.org
	HostName tty.sdf.org

Host ubnt
	HostName 192.168.1.20
	User ubnt
	UserKnownHostsFile /dev/null
	GlobalKnownHostsFile /dev/null
	StrictHostKeyChecking no
	ControlPersist no

# global options

Host *
	CanonicalizeHostname yes
	CanonicalDomains nullroute.eu.org nullroute.lt utenos-kolegija.lt ukolegija.lt
	Protocol 2
	HashKnownHosts no
	VerifyHostKeyDNS yes
	GSSAPIAuthentication yes
	SendEnv LANG TZ
	ControlPath ~/.ssh/S.%r@%h:%p
