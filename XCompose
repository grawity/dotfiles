# ~/.XCompose - Compose(5) - Compose (Multi) key mappings for XIM input method
# enabling compose key: <https://wiki.ubuntu.com/ComposeKey>

# NOTE: custom definitions only work with 'xim'
# see GTK_IM_MODULE in my "environ" file

# defaults: /usr/share/X11/locale/en_US.UTF-8/Compose

include "%L"

# letters

<Multi_key> <W> <W>			: "Ç·"	U017F # LATIN CAPITAL LETTER WYNN
<Multi_key> <w> <w>			: "Æ¿"	U01BF # LATIN LETTER WYNN
<Multi_key> <G> <G>			: "Èœ"	U021C # LATIN CAPITAL LETTER YOGH
<Multi_key> <g> <g>			: "È"	U021D # LATIN LETTER YOGH

<Multi_key> <v> <S>			: "Å "
<Multi_key> <v> <s>			: "Å¡"

<Multi_key> <n> <n>			: "ğ’"

# quotation marks

<Multi_key> <bracketleft> <less>	: "ã€ˆ"	U3008 # LEFT ANGLE BRACKET
<Multi_key> <bracketright> <greater>	: "ã€‰"	U3009 # RIGHT ANGLE BRACKET

<Multi_key> <bracketleft> <apostrophe>	: "ã€Œ"	U300C # LEFT CORNER BRACKET
<Multi_key> <bracketright> <apostrophe>	: "ã€"	U300D # RIGHT CORNER BRACKET

<Multi_key> <bracketleft> <parenleft>	: "ã€”"	U3014 # LEFT TORTOISE SHELL BRACKET
<Multi_key> <bracketright> <parenright>	: "ã€•"	U3015 # RIGHT TORTOISE SHELL BRACKET

<Multi_key> <bracketleft> <quotedbl>	: "ã€"
<Multi_key> <bracketright> <quotedbl>	: "ã€"

# various glyphs

<Multi_key> <equal> <equal>		: "â€“" # ndash - - .
<Multi_key> <l> <d> <a>			: "â€”â€”â€”â€”â€”" # 3x mdash
