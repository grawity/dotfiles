# ~/.XCompose - Compose(5) - Compose (Multi) key mappings for XIM input method
#
# Enabling compose key: <https://wiki.ubuntu.com/ComposeKey>
#
# NOTE: In GTK, custom definitions only work in 3.18+gitmaster (as of commit
#       a41f02f9b1843e0f0085f801430e55f413a9bf9c). For older versions, switch
#       to 'ibus' or 'xim' immodules (see GTK_IM_MODULE in my "environ" file)
#
# NOTE: IBus support for XCompose is incomplete ‚Äì only 2-letter shortcuts are
#       supported, e.g. "<Multi_key> <z> <w> <j>" won't work. However, stacked
#       <Multi_key> works fine.

# defaults: /usr/share/X11/locale/en_US.UTF-8/Compose

include "%L"

# letters

<Multi_key> <D> <A>			: "Œî"	U0394 # Delta
<Multi_key> <D> <a>			: "Œî"	U0394 # Delta
<Multi_key> <d> <a>			: "Œ¥"	U03B4 # delta
<Multi_key> <l> <a>			: "Œª"	U03BB # lambda
<Multi_key> <O> <M>			: "Œ©"	U03A9 # Omega
<Multi_key> <O> <m>			: "Œ©"	U03A9 # Omega
<Multi_key> <o> <m>			: "œâ"	U03C9 # omega
<Multi_key> <p> <i>			: "œÄ"	U03C0 # pi

<Multi_key> <n> <n>			: "ùíè"	#U1d48f
<Multi_key> <n> <quotedbl>		: "nÃà"	# SPINAL TAP

# various glyphs

<Multi_key> <equal> <equal>		: "‚Äì"	U2013 # EN DASH, - - .
<Multi_key> <zcaron> <zcaron>		: "‚Äì"	U2013 # EN DASH, - - .
<Multi_key> <e> <u>			: "‚Ç¨"	U20AC # EURO SIGN, e =
<Multi_key> <equal> <asciitilde>	: "‚âà"	U2248 # almost equal
<Multi_key> <asciitilde> <asciitilde>	: "‚âà"	U2248
<Multi_key> <less> <4>			: "‚ù§"	U2764
<Multi_key> <3> <greater>		: "‚ù•"	U2765
<Multi_key> <slash> <slash>		: "‚ÅÑ"	U2044 # FRACTION SLASH
<Multi_key> <v> <slash>			: "‚úî"	U2714 # check
<Multi_key> <ampersand> <ampersand>	: "‚àß"	U2227 # LOGICAL AND
<Multi_key> <bar> <bar>			: "‚à®"	U2228 # LOGICAL OR
<Multi_key> <minus> <period>		: "‚ãØ"	U22EF # MID HORIZ ELLIPSIS
<Multi_key> <bar> <period>		: "‚Åù"	U205D # TRICOLON
<Multi_key> <4> <period>		: "‚Åû"	U205E # VERTICAL FOUR DOTS
<Multi_key> <backslash> <backslash>	: "¬¶"	U00A6 # BROKEN BAR
<Multi_key> <bar> <greater>		: "‚ùØ"

<Multi_key> <Multi_key> <bar> <bar>		: "‚à©"	U2229 # INTERSECTION
<Multi_key> <Multi_key> <ampersand> <ampersand>	: "‚à™"	U222A # UNION

# quotation marks

<Multi_key> <l> <q>			: "‚Äú"	U201c # LEFT DOUBLE QUOT. MARK <"
<Multi_key> <r> <q>			: "‚Äù"	U201d # RIGHT DOUBLE QUOT. MARK >"

<Multi_key> <apostrophe> <period>	: "‚Äô"	U2019 # RIGHT SINGLE QUOT. MARK >'
<Multi_key> <period> <apostrophe>	: "‚Äô"	U2019 # -same-

<Multi_key> <bracketleft> <less>	: "„Äà"	U3008 # LEFT ANGLE BRACKET
<Multi_key> <bracketright> <greater>	: "„Äâ"	U3009 # RIGHT ANGLE BRACKET

<Multi_key> <bracketleft> <parenleft>	: "„Äî"	U3014 # LEFT TORTOISE SHELL BRACKET
<Multi_key> <bracketright> <parenright>	: "„Äï"	U3015 # RIGHT TORTOISE SHELL BRACKET

<Multi_key> <bracketleft> <apostrophe>	: "„Äå"	U300c # LEFT CORNER BRACKET
<Multi_key> <bracketright> <apostrophe>	: "„Äç"	U300d # RIGHT CORNER BRACKET

<Multi_key> <bracketleft> <quotedbl>	: "„Äé"	U300e
<Multi_key> <bracketright> <quotedbl>	: "„Äè"	U300f

<Multi_key> <Multi_key> <bracketleft> <apostrophe>	: "ÔΩ¢" Uff62 # HALF‚Ä¶ LEFT C B
<Multi_key> <Multi_key> <bracketright> <apostrophe>	: "ÔΩ£" Uff63 # HALF‚Ä¶ RIGHT C B

# invisible punctuation

<Multi_key> <n> <h> <y>			: U2011 # non-breaking hyphen
<Multi_key> <s> <h> <y>			: U00ad # 'hyphen' soft hyphen
<Multi_key> <z> <w> <j>			: U200d # zero-width joiner
<Multi_key> <z> <w> <n> <j>		: U200c # zero-width non-joiner
<Multi_key> <z> <w> <s>			: U200b # zero-width space

# http://unicode.org/reports/tr9/#Directional_Formatting_Characters

<Multi_key> <Multi_key> <l> <r> <e>	: U202a # left-to-right embedding
<Multi_key> <Multi_key> <l> <r> <m>	: U200e # left-to-right mark
<Multi_key> <Multi_key> <l> <r> <o>	: U202d # left-to-right override
<Multi_key> <Multi_key> <p> <d> <f>	: U202c # pop directional formatting
<Multi_key> <Multi_key> <r> <l> <e>	: U202b # right-to-left embedding
<Multi_key> <Multi_key> <r> <l> <m>	: U200f # right-to-left mark
<Multi_key> <Multi_key> <r> <l> <o>	: U202e # right-to-left override

# arrows

<Multi_key> <6> <6>			: "‚Üë" # up arrow
<Multi_key> <v> <v>			: "‚Üì" # down arrow

<Multi_key> <l> <r>			: "‚áÜ" # lrarr
<Multi_key> <r> <l>			: "‚áÑ" # rlarr

<Multi_key> <Return> <Return> 		: "‚Üµ"

# arrows on keypad

